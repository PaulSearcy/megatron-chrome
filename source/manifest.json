{
  "name": "Megatron",
  "manifest_version": 2,
  "version": "2",
  "description": "Opens a new tab and adds tfrm=4 to the current URL",
  "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js",
            "jquery-2.0.0.min.js"
        ]
    },
    "permissions": [
        "tabs",
        "http://*/",
        "https://*/"
    ]
}